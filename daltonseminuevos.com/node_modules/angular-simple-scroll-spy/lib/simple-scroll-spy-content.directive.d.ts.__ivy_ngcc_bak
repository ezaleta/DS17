import { ElementRef, EventEmitter } from '@angular/core';
export declare class ScrollSpyContentDirective {
    private _element;
    target: any;
    private _currentContent;
    sscCurrentContent: string;
    sscCurrentContentChange: EventEmitter<string>;
    sscDirection: "row" | "colmun";
    constructor(_element: ElementRef);
    onScroll(event: any): void;
}
