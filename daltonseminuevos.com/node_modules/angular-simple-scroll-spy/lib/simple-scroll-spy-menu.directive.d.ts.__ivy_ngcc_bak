import { ElementRef, Renderer2 } from '@angular/core';
export declare class ScrollSpyMenuDirective {
    private _element;
    private _renderer;
    contentId: string;
    ssmActiveClassName: string;
    ssmCurrentContent: string;
    ssmScrollBehavior: ScrollBehavior;
    ssmScrollBlock: ScrollLogicalPosition;
    ssmScrollInline: ScrollLogicalPosition;
    constructor(_element: ElementRef, _renderer: Renderer2);
    onClick(event: any): void;
}
